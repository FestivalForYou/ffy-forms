    "url": "https://github.com/FestivalForYou/ffy-forms.git"
  },
  "author": "Phil Wiltsher",
  "license": "GPL-3.0",
  "dependencies": {
    "react": "^15.5.4"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1"
  }
}
